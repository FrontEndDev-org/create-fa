{
  "name": "<%= ctx.projectName %>",
  "version": "1.0.0",
  "description": "A new React App - <%= ctx.projectName %>",
  "scripts": {
    "prepare": "lefthook install",
    <%_ if (codeLinter === 'biome') { _%>
    "lint": "biome check && tsc --noEmit && stylelint '**/*.{css,scss}'",
    "lint:fix": "biome check --fix --unsafe && tsc --noEmit && stylelint '**/*.{css,scss}' --fix",
    <%_ } else if (codeLinter === 'eslint') { _%>
    "lint": "eslint && tsc --noEmit && stylelint '**/*.{css,scss}'",
    "lint:fix": "eslint --fix && tsc --noEmit && stylelint '**/*.{css,scss}' --fix",
    <%_ } _%>
    "dev": "cross-env NODE_ENV=development vite dev",
    "build": "cross-env NODE_ENV=production vite build",
    "preview": "cross-env NODE_ENV=production vite preview",
    "release": "commit-and-tag-version"
  },
  "engines": {
    "node": ">=<%= nodeVersion %>"
  },
  "engineStrict": true,
  "dependencies": {
    "@generouted/react-router": "^1.19.10",
    "@unocss/reset": "^65.4.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.1.3",
    "valtio": "^2.1.2"
  },
  "devDependencies": {
    <%_ if (codeLinter === 'biome') { _%>
    "@biomejs/biome": "1.9.4",
    <%_ } else if (codeLinter === 'eslint') { _%>
    "@antfu/eslint-config": "^3.14.0",
    "eslint": "^9.18.0",
    "prettier": "^3.4.2",
    <%_ } _%>
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@commitlint/types": "^19.5.0",
    "@types/node": "<%= nodeVersion %>",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@unocss/postcss": "^65.4.3",
    "@vitejs/plugin-react": "^4.3.4",
    "commit-and-tag-version": "^12.5.0",
    "cross-env": "^7.0.3",
    "lefthook": "^1.10.3",
    "sass-embedded": "^1.83.4",
    "stylelint": "^16.13.2",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended": "^15.0.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-standard": "^37.0.0",
    "typescript": "^5.7.3",
    "unocss": "^65.4.0",
    "vite": "^6.0.7",
    "vite-plugin-ejs": "^1.7.0",
    "vite-plugin-minify": "^2.1.0",
    "vite-svg-sprite": "^1.0.1",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
